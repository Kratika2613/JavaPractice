//============================================================================================================================
import java.util.*;
public class Stringjava{
    public static void main(String[]args){
        int[] arr1 = {1,3,5};
        int[] arr2 = {2,4,6};
        int[]mergedArray = new int[arr1.length + arr2.length];
        for(int i=0; i<arr1.length; i++){
            mergedArray[i] = arr1[i];
        }
        for(int i=0; i<arr1.length; i++){
            mergedArray[arr1.length + i] = arr2[i];
        }
        Arrays.sort(mergedArray);
        System.out.println("The merged & sorted array is:"+ Arrays.toString(mergedArray));
        System.out.println("The median of this array is:"+ findMedian(mergedArray));
        
    }
    public static double findMedian(int[]mergedArray){
        int length = mergedArray.length;
        int mid = (length/2) ;
        if(length % 2 == 0){
            double median = (mid + mid-1)/2;
            return median;
        }
        else{
            return mergedArray[mid];
        } 
    }
}

