 /* import java.util.Scanner;

 public class arraypr {
     public static void main(String[] args) {
         Scanner sc = new Scanner(System.in);
         int rev = 0;
         int num = sc.nextInt();
         int n = num;
         for (;num > 0; num = num / 10) {
             num = num % 10;
             rev = (rev * 10) + n;
            
         }
 
         if (n == rev ) {
             System.out.println("Palindrome");
         } else {
             System.out.print("Not palindrome");
         }
     }
